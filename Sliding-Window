class Solution {
    public int numSubarrayProductLessThanK(int[] nums, int k) {
        int ei=0;
        int si=0;
        int p=1;
        int count=0;
        int i=0;
        while(ei<nums.length){
            p=p*nums[ei];
           while(p>=k && si<=ei){
            p=p/nums[si];
            si++;
}
           count=count+(ei-si+1);
           ei++;
           }
return count;
    }
}
